<!--js files-->
 Version 1

var askQuestion = prompt("Are we there yet?")
while(askQuestion !== "yes" && askQuestion !== "yeah"){
	var askQuestion = prompt("Are we there yet?");
}
alert("Yes we made it!!!");

Version 2

var askQuestion = prompt("Are we there yet?")
while(askQuestion.indexOf("yes") === -1){
	var askQuestion = prompt("Are we there yet?");
}
alert("Yes we made it!!!");









